#!/bin/rc -e
git
cd /
rm -f /sys/src/cmd/rio/menuhit.c* /sys/src/cmd/rio/col.h*
git/revert /sys/src/^(libdraw games/sudoku cmd/^(acme colors.c faces fplot.c kbmap.c mothra sam* spred stats.c aux/status* vt winwatch.c audio/zuke rio))
exit nope: deprecated patch
pa4 $home/p/patch/^(\
	sam-sharedsnarf\
	samterm-flayers\
	samterm-fullfrontalnudity\
	samterm-mousefocus\
	samterm-pastelcol\
	libdraw-pastelmenucol\
)
pa4 $home/p/patch/pastels
ramfs
mkdir /tmp/lib
dircp /amd64/lib /tmp/lib
bind -bc /tmp/lib /amd64/lib
cd /sys/src/libdraw && mk nuke && mk install && mk clean
cd /
mkdir -p /tmp/bin/^(aux games audio)
cd /sys/src/cmd/rio && mk && mv 6.out /tmp/bin/rio && mk clean
cd /sys/src/cmd/samterm && mk && mv 6.out /tmp/bin/aux/samterm && mk clean
cd /sys/src/cmd/mothra && mk && mv 6.out /tmp/bin/aux/mothra && mk nuke
cd /sys/src/cmd/acme && mk && mv 6.out /tmp/bin/acme && mk clean
cd /sys/src/cmd/faces && mk && mv 6.out /tmp/bin/faces && mk clean
cd /sys/src/cmd/spred && mk && mv 6.out /tmp/bin/spred && mk clean
cd /sys/src/cmd/vt && mk && mv 6.out /tmp/bin/vt && mk clean
cd /sys/src/games/sudoku && mk && mv 6.out /tmp/bin/games/sudoku && mk clean
cd /sys/src/cmd/audio/libtags && mk
cd /sys/src/cmd/audio/zuke && mk && mv 6.zuke /tmp/bin/audio/zuke && mk clean
cd /sys/src/cmd/audio/libtags && mk clean
cd /sys/src/cmd && mk 6.stats && mv 6.stats /tmp/bin/stats
cd /sys/src/cmd && mk 6.winwatch && mv 6.winwatch /tmp/bin/winwatch
cd /sys/src/cmd && mk 6.fplot && mv 6.fplot /tmp/bin/fplot
cd /sys/src/cmd && mk 6.colors && mv 6.colors /tmp/bin/colors
cd /sys/src/cmd && mk 6.kbmap && mv 6.kbmap /tmp/bin/kbmap
cd /sys/src/cmd/aux && mk 6.statusmsg && mv 6.statusmsg /tmp/bin/aux/statusmsg
cd /sys/src/cmd/aux && mk 6.statusbar && mv 6.statusbar /tmp/bin/aux/statusbar
cd /sys/src/cmd/aux && mk clean
cd /sys/src/cmd && mk cleancmds
cd /sys/lib/extra/usr/spew/aplay && mk 6.volume && mv 6.volume /tmp/bin/volume && mk clean
cd /sys/lib/extra/usr/sigrid/bar && mk 6.bar && mv 6.bar /tmp/bin/bar && mk clean
dircp /tmp/bin /usr/qwx/pastel
git/revert /sys/src/^(libdraw games/sudoku cmd/^(acme colors.c faces fplot.c kbmap.c mothra sam* spred stats.c aux/status* vt winwatch.c audio/zuke rio))
