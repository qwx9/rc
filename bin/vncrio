#!/bin/rc
rfork nefs
service=terminal
norio=1
if(test -f $home/lib/ass){
	auth/factotum
	setec
}
. $home/lib/profile
cat <<! >/tmp/bof.$pid
#!/bin/rc
window -r 586 0 1280 584 9grid
window -scroll -r 607 749 1280 800 girc '/tmp/irc.irc.oftc.net!#cat-v'
window stats -lmse
window 'label wii; echo wii; rc'
!
chmod +x /tmp/bof.$pid
bof -i /tmp/bof.$pid
rm /tmp/bof.$pid
