#!/bin/rc
font=/lib/font/bit/vga/unicode.font
bind -c $home/tmp /tmp
bind -a $home/bin/rc /bin
bind -a $home/bin/$cputype /bin
if(test -d $home/p/rc)
	bind -a $home/p/rc /bin
if(test -f $home/p/dot/fn)
	. $home/p/dot/fn
if(test -f /tmp/sk){
	auth/factotum
	auth/aescbc -d </tmp/sk | read -m >/mnt/factotum/ctl
}
webcookies
webfs
plumber
echo accelerated 5 >'#m'/mousectl
prompt=('; ' '	')
rio
